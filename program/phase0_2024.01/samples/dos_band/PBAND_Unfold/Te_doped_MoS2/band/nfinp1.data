control{
    condition = fixed_charge
    sw_vdw_correction = on
    fixed_charge_option{
        kparallel = one_by_one
    }
}
accuracy{
    cutoff_wf = 25 Rydberg
    cutoff_cd = 225 Rydberg

    ksampling{
        method = file
    }
    num_bands = 160
    scf_convergence{
        delta_total_energy = 1.0E-8 Hartree
    }
    force_convergence{
        max_force = 2.0E-4 Hartree/Bohr
    }
    smearing{
           method = parabolic ! {parabolic|tetrahedral}
           width  = 0.05 eV
    }
    paw = on
    initial_wavefunctions = atomic_orbital
    initial_charge_density = atomic_charge

    vdw_method = grimme
    projector_list{
        projectors{
            #tag no  group radius l
                 1   1    2.3   0
                 2   1    2.3   1
                 3   1    2.3   2
             }
    }
}
